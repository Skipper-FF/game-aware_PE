<h1>This is the user_review page for the edit</h1>
<div class="container">
<%= simple_form_for [@user_review] do |f| %>
  <%= f.input :age, collection: 0..18 %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :rating, collection: 0..5 %>
  <%= f.submit'Edit your comment', class: "btn btn-primary" %>
<% end %>
</div>
